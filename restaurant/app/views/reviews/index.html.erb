<div class='container-fluid'>
  <div class='row'>
    <div class='col-lg-11'>
    <div class="card">
    <div class="card-header">
      <h1>Reviews</h1></div>
      <% @reviews.each do |review| %>
        <div class="card-body">
          <table>
          <tbody>
          <tr>
          <td><h3 class="card-title">
          <%= review.resturant.name %></h3>
          </td>
          </tr>
          <tr>
          <td><h4 class="card-title"><%= review.title %></h4></td>
          </tr>
          <tr>
          <td><p class="card-text"><%= review.body %></p><br></td>
          </tr>
          <tr>
          <td><p class="blockquote-footer"><%= review.user.email %></p></td>
          </tr>
          <tr><br></tr>
          </tbody>
          </table>
        </div>
      <% end %>
      <%= paginate @reviews %>
      <br>
      </div>
    </div>
  </div>
</div>